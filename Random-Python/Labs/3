# Phay Vong 

def test_number(a: int, test: str) -> bool:
  if test == 'even':
    return a % 2 == 0
  if test == 'odd':
    return a % 2 != 0
  if test == 'positive':
    return a > 0
  if test == 'negative':
    return a < 0
    
assert test_number(14, 'even')
assert not test_number(100, 'odd')
assert test_number(33, 'positive')
assert not test_number(100, 'negative')


print(test_number(10, 'even'))
print(test_number(-10, 'positive'))            

print('---------Problem 2---------')

def display():
  word = input('Enter a word: ')
  for letters in word:
    print(letters)

display()

print('---------Problem 3---------')
def square_list(integers: list) -> int:
  for numbers in integers:
    print(numbers**2)
    
square_list([1,2,3,4,5])


print('---------Problem 4---------')
def match_first_letter(a: str, l: list):
    for movie in l:
      if movie[0] == a:
        print(movie)

match_first_letter('I', ['Iron Man', 'Iron Man 2', 'The Avengers', 'Superman', 'I am Legend'])

print('---------Problem 5---------')
def match_area_code(ac: list, pn: list):
  for numbers in pn:
    if numbers[1:4] in ac:
      print(numbers)


match_area_code(['949','714'], ['(714)824-1234', '(419)312-8732', '(949)555-1234'])

print('---------Problem 6---------')
def matching_area_codes(ac: list, pn: list):
  phonenum = []
  for numbers in pn:
    if numbers[1:4] in ac:
      phonenum.insert(0,numbers)
  return phonenum

print(matching_area_codes(['949','714'], ['(714)824-1234', '(419)312-8732', '(949)555-1234']))

print('---------Problem d1---------')
def is_vowel(letter: str) -> bool:
  vowels = ['a','e','i','o','u','A','E','I','O','U']
  return letter in vowels

print(is_vowel('a'))
print(is_vowel('b'))


print('---------Problem d2---------')
def print_nonvowels(s: str) -> str:
  vowels = ['a','e','i','o','u','A','E','I','O','U']
  for letters in s:
    if letters not in vowels:
      print(letters)

print_nonvowels('Apple')

print('---------Problem d3---------')
def nonvowels(s: str) -> str:
  result = ''
  vowels = ['a','e','i','o','u','A','E','I','O','U']
  for letters in s:
    if letters not in vowels:
      result += letters
  return result

print(nonvowels('Apple'))


print('---------Problem d4---------')
def constants(s: str) -> str:
  result = ''
  consts = 'bBcCdDfFgGhHjJkKlLmMnNpPqQrRsStTvVwWxXyYzZ'
  for i in s:
      if i in consts:
        result += i
  return result

print(constants('APple'))


print('---------Problem d5---------')
def select_letters(sel: str, lect: str) -> str:
    vowels= 'aeiouAEIOU'
    consonants = 'bBcCdDfFgGhHjJkKlLmMnNpPqQrRsStTvVwWxXyYzZ'
    v = ''
    c = ''
    if sel == 'v':
        for i in lect:
            if i in vowels:
                v += i
        return v
    if sel == 'c':
        for i in lect:
            if i in consonants:
                c += i
        return c
    return '' #or None? empty string

print('This should be vowels',select_letters('v', 'Apple'))
print('This should be constants',select_letters('c', 'Apple'))


print('---------Problem d6---------')
def hide_vowels(word:str) -> str:
    result = ''
    vowels = 'aeiou'
    for letters in word:
        if letters in vowels:
            result += '-'
        else:
            result += letters
    return result
  

print(hide_vowels('Face'))



from collections import namedtuple

Restaurant = namedtuple('Restaurant', 'name cuisine phone dish price')
# Restaurant attributes: name, kind of food served, phone number,
#	best dish, price of that dish

