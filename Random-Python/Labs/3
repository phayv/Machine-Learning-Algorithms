	R17, R18, R19, R20, R21, R22, R23, R24, R25, R26]


print('---------Problem e---------')
def Restaurant_change_price(s: Restaurant, f: float) -> Restaurant:
  s = s._replace(price = s.price + f)
  return s

print(Restaurant_change_price(R26, 3.5))


print('---------Problem f1---------')
def alphabetical(l: list) -> list:
  l.sort()
  return l
  
print(alphabetical(RL))


print('---------Problem f2---------')
def alphabetical_names(l: list) -> list:
  results = []
  l.sort()
  for restaurants in l:
    results.append(restaurants.name)
  return results

print(alphabetical_names(RL))


print('---------Problem f3---------')
def all_Thai(l: list) -> list:
  results = []
  for stuff in l:
    if stuff.cuisine == 'Thai':
      results.append(stuff.name)
  return results

print(all_Thai(RL))


print('---------Problem f4---------')
def select_cuisine(l: list, s: str) -> str:
  results = []
  for stuff in l:
    if stuff.cuisine == s:
      results.append(stuff)
  return results

print(select_cuisine(RL,'Thai'))



print('---------Problem f5---------')
def select_cheaper(l: list, n: float) -> list:
  results = []
  for stuff in l:
    if stuff.price < n:
      results.append(stuff.name)
  return results

print(select_cheaper(RL,10.50))


print('---------Problem f6---------')
def average_price(l:list) -> float:
  results = 0
  n = len(l)
  for stuff in l:
    results += stuff.price
  average = results/n
  return average

print(average_price(RL))
  

print('---------Problem f7---------')
print(average_price(select_cuisine(RL,'Indian')))


print('---------Problem f8---------')
print(average_price(select_cuisine(RL,'Chinese')+select_cuisine(RL,'Thai')))


print('---------Problem f9---------')
print(select_cheaper(RL,15))


print('---------Problem g---------')
import tkinter
my_window = tkinter.Tk() #created the window
my_canvas = tkinter.Canvas(my_window, width = 500, height = 500)
#created a 500x500 size canvas
my_canvas.pack() #putting canvas into the created window earlier


def create_rectangle_from_center(x:int,y:int,height:int,width:int):
  my_canvas.create_rectangle(x-width/2,y-height/2,x+width/2,y+height/2)


create_rectangle_from_center(250,250,300,200)
